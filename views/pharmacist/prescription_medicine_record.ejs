<modal title="Medicine List" visible="showMedicineListModal">
	<label>Doctor: </label>
	<table class="table table-striped">
		<thead>
			<tr>
				<th>Medicine List</th>
				<th>Dosage</th>
				<th>How to</th>
			</tr>
		</thead>
		<tbody>
			<tr ng-repeat="a in [1,2,3]">
				<td>{{a}}</td>
				<td>{{a}}</td>
				<td>{{a}}</td>
			</tr>
		</tbody>
	</table>
	<button type="submit" id="submit" class="btn btn-primary">Submit</button>
</modal>